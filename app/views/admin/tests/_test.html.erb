<tr>
  <td>
    <%= content_tag :span, test.title, class: 'test-title', data: { test_id: test.id } %>
    <%= render 'form_inline', test: test == @test ? @test : test %>
  </td>
  <td>
    <%= test_count(test) %>
  </td>
  <td>
    <%= link_to 'show', admin_test_path(test) %>
    <%= link_to 'edit', '#', data: { test_id: test.id }, class: 'form-inline-link' %>
    <%= button_to 'start', start_test_path(test) %>
  </td>
</tr>
