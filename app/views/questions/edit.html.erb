<%= form_with model: @question do |form| %>
  <% @test = @question.test %>
  <h3><%= question_header @question %></h3>
  <div>
    <%= form.label :description %>
    <%= form.text_field :description %>
  </div>
  <br>
  <%= form.submit %>
<% end %>
<%= link_to 'Back to test questions', test_path(@question.test_id) %>
