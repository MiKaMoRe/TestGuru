<%= form_with model: [@test, @question] do |form| %>
  <h3><%= question_header :create, @test %></h3>
  <div>
    <%= form.label :description %>
    <%= form.text_field :description %>
  </div>
  <br>
  <%= form.submit %>
<% end %>
<%= link_to 'Back to test questions', test_questions_path(@test) %>
