<tr>
  <th>
    <%= question.description %>
  </th>
  <th>
    <%= link_to 'show', question_path(question) %>
    <%= link_to 'edit', edit_question_path(question) %>
    <%= 
      link_to 'destroy', 
      question_path(question), 
      method: :delete, 
      data: { confirm: 'Are you sure?' } 
    %>
  </th>
</tr>
